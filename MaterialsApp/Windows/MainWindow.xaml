<Window x:Class="MaterialsApp.Windows.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MaterialsApp.Windows"
        mc:Ignorable="d"
        Title="HandWear" Height="600" MinWidth="800" Width="1000" FontFamily="{DynamicResource MainFont}" Icon="/Pictures/Одежда для ручек.ico">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="35"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <StackPanel VerticalAlignment="Center" Orientation="Horizontal" Margin="10 0 0 0">
            <Image Source="/Pictures/Одежда для ручек.png" Height="25" />
            <Label Content="Одежда для ручек"/>
        </StackPanel>
        <Frame Grid.Row="1" x:Name="baseFrame" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
